<h1>Submit</h1>

<%= render("layouts/errors", :object => @link) %>

<%= form_for @link do |f| %>
  <div class="field">
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </div>
  <div class="field">
    <%= f.label :url %><br />
    <%= f.password_field :url %>
  </div>

  <div class="actions"><%= f.submit "Submit" %></div>
<% end %>
